{
  "name": "ng-cue-cards",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run sass2json-sb && npm run sass2json-cc && ng serve",
    "build": "npm run sass2json-sb && npm run sass2json-cc && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sass2json-sb": "sass-export ./src/app/components/cue-card-shoe-box/cue-card-shoe-box.component.scss -o ./src/app/generated/styles/cue-card-shoe-box.ts && npm run json2module-sb",
    "json2module-sb": "sed -i '1i export default' ./src/app/generated/styles/cue-card-shoe-box.ts",
    "sass2json-cc": "sass-export ./src/styles/cue-card.scss -o ./src/app/generated/styles/cue-card.ts && npm run json2module-cc",
    "json2module-cc": "sed -i '1i export default' ./src/app/generated/styles/cue-card.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "reduce-css-calc": "^2.1.6",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "sass-export": "^1.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
